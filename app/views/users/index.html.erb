

<%= link_to 'New User', new_user_path %>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.first_name  %></td>
        <td><%= user.last_name  %></td>
      </tr>
    <% end %>
  </tbody>
</table>